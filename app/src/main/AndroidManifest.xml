<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.comma.blankcomma" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".ParseApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".FirstFlow"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name=".AddNoteActivity"
            android:label="@string/title_activity_add_note" >
        </activity>
        <activity
            android:name=".SearchListActivity"
            android:label="@string/title_activity_search_list"
            android:theme="@android:style/Theme.Dialog" >
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginSignupActivity" >
        </activity>
        <activity android:name=".Welcome" >
        </activity>
        <activity android:name=".SignupActivity" >
        </activity>
        <activity
            android:name=".MainFlowActivity"
            android:label="@string/title_activity_main_flow" >
        </activity>
    </application>

</manifest>
